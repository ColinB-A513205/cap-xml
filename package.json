{
  "name": "cap-xml",
  "version": "1.0.0",
  "description": "Serverless (AWS Lambda) project for CAP XML service",
  "engines": {
    "node": ">12"
  },
  "main": "handler.js",
  "scripts": {
    "deploy": "npm run validate-config && sls deploy -v --force",
    "lint": "standard",
    "unit-test": "lab test -v -c --coverage-exclude lib/helpers -r console -o stdout -r lcov -o lcov.info -m 5000",
    "test": "npm run lint && npm run unit-test",
    "validate-config": "node ./config/index.js"
  },
  "author": "The Environment Agency",
  "license": "OGL",
  "dependencies": {
    "feed": "1.1.1",
    "joi": "13.3.0",
    "moment": "2.22.1",
    "pg": "7.4.3",
    "sql": "0.78.0",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "@hapi/code": "^7.0.0",
    "@hapi/lab": "^21.0.0",
    "code": "5.2.0",
    "codeclimate-test-reporter": "0.5.1",
    "lab": "15.4.5",
    "serverless": "1.27.2",
    "standard": "11.0.1"
  }
}
